run = "cd demos/ack-lab && ./bin/run-replit.sh"

[deployment]
run = ["sh", "-c", "cd demos/ack-lab && ./bin/run-replit.sh"]
deploymentTarget = "cloudrun"

[nix]
channel = "stable-24_05"

[env]
XDG_CONFIG_HOME = "$REPL_HOME/.config"
npm_config_prefix = "$XDG_CONFIG_HOME/npm/node_global"
PATH = "$npm_config_prefix/bin:$REPL_HOME/node_modules/.bin"
PNPM_HOME = "$XDG_CONFIG_HOME/pnpm"

[packager]
language = "nodejs-pnpm"

[packager.features]
packageSearch = true
guessImports = false
enabledForHosting = false

[[ports]]
localPort = 3000
externalPort = 80

[[ports]]
localPort = 3001
externalPort = 5173

[[ports]]
localPort = 5677
externalPort = 3000

[[ports]]
localPort = 5678
externalPort = 3001

[[ports]]
localPort = 5679
externalPort = 3002

[[ports]]
localPort = 5680
externalPort = 3003

[[ports]]
localPort = 5681
externalPort = 4200

[[ports]]
localPort = 5682
externalPort = 5000
